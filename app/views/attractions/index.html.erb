<h1>Attractions</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <tr>
      <!-- <td><strong>Picture</strong></td> -->
      <td><strong>Name</strong></td>
      <td><strong>Tickets</strong></td>
      <td><strong>Details</strong></td>
    </tr>

    <% @attractions.each do |attraction| %>
      <tr>
        <td><%= attraction.name %>
        <td><%= attraction.tickets %>
        <td>Ride the <%= link_to attraction.name, attraction %>
      </tr>
      <% end %>

  </tbody>
</table>
