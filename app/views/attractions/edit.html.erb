<%= form_for @attraction do |f| %>
<br><%= f.label :name %>
<%= f.text_field :name, placeholder: "Name" %>

<br><%= f.label :tickets %>
<%= f.number_field :tickets, placeholder: "Tickets" %>

<br><%= f.label :nausea_rating %>
<%= f.number_field :nausea_rating, placeholder: "Nausea Rating"%>

<br><%= f.label :happiness_rating %>
<%= f.number_field :happiness_rating, placeholder: "Happiness Rating" %>

<br><%= f.label :min_height %>
<%= f.number_field :min_height, placeholder: "Min Height" %>

<br><%= f.submit %>

<% end %>
