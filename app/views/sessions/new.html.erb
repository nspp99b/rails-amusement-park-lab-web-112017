<% if flash[:error] != nil %>
  <%flash[:error].each do |e| %>
    <%= e %>
  <% end %>
<% end %>

<%= form_tag sessions_path do %>
	<%= text_field_tag :name, nil, placeholder: "name" %>
	<%= password_field_tag :password, nil, placeholder: "password" %>
	<%= submit_tag "Sign In" %>
<% end %>

New User? <%= link_to "Sign Up", signup_path %>
